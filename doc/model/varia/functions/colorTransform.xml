<?xml version="1.0" encoding="UTF-8"?>
<c:model 
    xmlns:c="urn:webdetails/com/2012" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="urn:webdetails/com/2012 ../../../schema/com_2012.xsd"
    xmlns="http://www.w3.org/1999/xhtml">
    
    <c:functionType name="ColorTransform" space="pvc.options.varia">
        <c:documentation>
            A function that applies an effect to 
            an input color and returns the resulting color.
        </c:documentation>
        <c:argument name="this" type="null" required="true">
            <c:documentation>
                The global JavaScript object.
            </c:documentation>
        </c:argument>
        <c:argument name="color" type="pv.Color" required="true">
            <c:documentation>
                The color to transform.
            </c:documentation>
        </c:argument>
        <c:returns type="pv.Color">
            <c:documentation>
                The transformed color.
            </c:documentation>
        </c:returns>
    </c:functionType>
    
</c:model>