<?xml version="1.0" encoding="UTF-8"?>
<c:model 
    xmlns:c="urn:webdetails/com/2012" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="urn:webdetails/com/2012 ../../../schema/com_2012.xsd"
    xmlns="http://www.w3.org/1999/xhtml">
    
    <c:atomType name="ContentOverflow" space="pvc.options.varia" base="string">
        <c:documentation>
            Controls what to do with content that overflows.
        </c:documentation>
        
        <c:atom name="Auto" value="'auto'">
            <c:documentation>
                Content is hidden only if one of the cartesian axis of a plot
                has its <tt>fixedMin</tt>, <tt>fixedMax</tt>, <tt>fixedLength</tt>, <tt>ratio</tt>
                or <tt>preserveRatio</tt> options specified.
            </c:documentation>
        </c:atom>
        
        <c:atom name="Hidden" value="'hidden'">
            <c:documentation>
                Content that overflows is hidden.  
            </c:documentation>
        </c:atom>

        <c:atom name="Visible" value="'visible'">
            <c:documentation>
                Content that overflows is visible.
            </c:documentation>
        </c:atom>
    </c:atomType>
</c:model>